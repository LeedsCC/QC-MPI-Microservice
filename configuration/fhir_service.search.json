{
  "Patient": {
    "nhsNumber": "identifier=https://fhir.nhs.uk/Id/nhs-number|{{ Patient.nhsNumber }}"
  },
  "PractitionerRole": {
    "practitionerReference": "practitioner={{ PractitionerRole.practitionerReference }}"
  },
  "Policy": {
    "name": "name:exact={{ Policy.name }}"
  },
  "Consent": {
    "consentor": "consentor={{ Consent.consentor }}"
  }
}